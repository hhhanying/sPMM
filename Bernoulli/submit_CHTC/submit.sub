universe=docker
docker_image=hanyingj/pymc3-sklearn

executable=MCMC.sh
transfer_input_files=configurations.csv,CV.py

# requesting resources
request_memory=10GB
request_cpus=1
request_disk=10GB

# parameters
environment="id=$(Step)"

# log files
error=err$(Process).err
output=out$(Process).out
log=log$(Process).log
queue 765
